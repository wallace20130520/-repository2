		<typegroup name="BunningsCategoryGroup">
            <itemtype code="Brand" generate="true" autocreate="true" extends="GenericItem">
                <deployment table="Brands" typecode="16062"/>
                <custom-properties>
                    <property name="catalogItemType">
                        <value>java.lang.Boolean.TRUE</value>
                    </property>
                    <property name="catalogVersionAttributeQualifier">
                        <value>"catalogVersion"</value>
                    </property>
                    <property name="uniqueKeyAttributeQualifier">
                        <value>"name"</value>
                    </property>
                    <property name="catalog.sync.default.root.type">
                        <value>Boolean.TRUE</value>
                    </property>
                </custom-properties>
                <attributes>
                    <attribute qualifier="brandCode" type="java.lang.String">
                       <description>unique code </description>
                        <modifiers read="true" write="true" search="true"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="catalogVersion" type="CatalogVersion">
                        <modifiers read="true" write="true" search="true" optional="false" unique="true" initial="true"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="name" type="java.lang.String">
                        <description>Name for the brand.</description>
                        <modifiers read="true" write="true" unique="true" initial="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                     <attribute qualifier="description" type="localized:java.lang.String">
                        <modifiers read="true" write="true" optional="true"/>
                        <defaultvalue>Description</defaultvalue>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="isLeadingBrand" type="boolean">
                        <description>Is this brand leading brand</description>
                        <defaultvalue>FALSE</defaultvalue>
                        <modifiers read="true" write="true" search="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="isTradeBrand" type="boolean">
                        <description>Is this brand trade brand</description>
                        <defaultvalue>FALSE</defaultvalue>
                        <modifiers read="true" write="true" search="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="isMarketPlaceBrand" type="boolean">
                        <description>Is this market link brand</description>
                        <defaultvalue>FALSE</defaultvalue>
                        <modifiers read="true" write="true" search="true" optional="false"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="logo" type="Media">
                        <description>List of logo visible on the web for the brand.</description>
                        <modifiers read="true" write="true"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="logoDescription" type="java.lang.String">
                        <description>Description of the web logo for the brand.</description>
                        <modifiers read="true" write="true"/>
                        <persistence type="property">
                            <columntype database="oracle">
                                <value>CLOB</value>
                            </columntype>
                            <columntype database="sap">
                                <value>NCLOB</value>
                            </columntype>
                            <columntype>
                                <value>HYBRIS.LONG_STRING</value>
                            </columntype>
                        </persistence>
                    </attribute>
                    <attribute qualifier="brandStatus" type="BrandStatusEnum">
                        <description>Current status for the brand.</description>
                        <modifiers read="true" write="true" optional="false"/>
                        <defaultvalue>em().getEnumerationValue("BrandStatusEnum","CHECK")</defaultvalue>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="feedLineReference" type="java.lang.String">
                        <description>Feed Line Reference</description>
                        <modifiers read="true" write="true" search="true" optional="true"/>
                        <persistence type="property"/>
                    </attribute>
                    <attribute qualifier="siteCoreItemID" type="java.lang.String">
						<modifiers read="true" write="true" search="true"
							optional="true" />
						<persistence type="property" />
					</attribute>
					<attribute qualifier="isDAMSyncRequired"
						type="java.lang.Boolean">
						<defaultvalue>Boolean.TRUE</defaultvalue>
						<modifiers read="true" write="true" search="true"
							optional="false" />
						<persistence type="property" />				
					</attribute>
					 
                </attributes>
            </itemtype>

